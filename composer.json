{
    "name": "oqlf/oqlf_ratelimit",
    "description": "Typo3 middleware limitting the number of requests over time a client can make",
    "authors": [
        {
            "name": "RÃ©mi Payette",
            "email": "rpayette@oqlf.gouv.qc.ca",
            "role": "Developer"
        }
    ],
    "type": "typo3-cms-extension",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "php": ">=7.0",
        "typo3/cms-core": "^13.4"
    },
    "replace": {
        "typo3-ter/oqlf_ratelimit": "self.version"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "OQLF\\ratelimit\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "oqlf_ratelimit"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
